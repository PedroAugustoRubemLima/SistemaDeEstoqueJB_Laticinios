<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.seuprojeto.lojadesktop.controller.HistoricoVendasController">

    <children>

        <Label layoutX="14" layoutY="14" text="Histórico de Vendas"/>

        <TableView fx:id="vendasTable" layoutX="14" layoutY="40" prefWidth="600" prefHeight="300">
            <columns>
                <TableColumn fx:id="colCliente" text="Cliente" prefWidth="100"/>
                <TableColumn fx:id="colFuncionario" text="Funcionário" prefWidth="100"/>
                <TableColumn fx:id="colData" text="Data" prefWidth="100"/>
                <TableColumn fx:id="colValorTotal" text="Valor Total" prefWidth="100"/>
            </columns>
        </TableView>

        <Button layoutX="14" layoutY="350" text="Fechar" onAction="#fechar"/>

    </children>
</AnchorPane>
